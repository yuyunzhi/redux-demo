ubscribe的回调函数就会触发\n    store.subscribe(()=>{\n      setListData(store.getState()) // {inputValue,list}\n    })\n  },[])\n\n\n  const handleInputChange = (e)=>{\n    const action = getInputChangeAction(e.target.value)\n    setInputValue(e.target.value)\n    store.dispatch(action)\n  }\n  const submitInput = ()=>{\n    const action = addInputListAction(inputValue)\n    setInputValue('')\n    store.dispatch(action)\n  }\n\n  const deleteItem = (index)=>{\n    const action = deleteInputListAction(index)\n    store.dispatch(action)\n  }\n\n  return (\n      <div>\n        <Space>\n          <Input placeholder=\"todo info\"\n                 onChange={handleInputChange}\n                 value={listData.inputValue} style={{width:300}}/>\n          <Button type=\"primary\" onClick={submitInput}>提交</Button>\n        </Space>\n        <List\n            bordered\n            style={{width:300,marginTop:10}}\n            dataSource={listData.list}\n            renderItem={(item,index)=>(<List.Item className=\"item\" onClick={()=>deleteItem(index)}>{item}</List.Item>)}\n        />\n\n      </div>\n  )\n}\n\nexport default Todolist\n","/Users/yuyunzhi/Desktop/jianshu/src/store/reducer.js",["28"],"/Users/yuyunzhi/Desktop/jianshu/src/store/index.js",[],"/Users/yuyunzhi/Desktop/jianshu/src/store/acitonType.js",[],"/Users/yuyunzhi/Desktop/jianshu/src/store/actionCreators.js",[],"/Users/yuyunzhi/Desktop/jianshu/src/App.js",[],{"ruleId":"29","severity":1,"message":"30","line":17,"column":5,"nodeType":"31","endLine":17,"endColumn":7,"suggestions":"32"},{"ruleId":"33","severity":1,"message":"34","line":10,"column":1,"nodeType":"35","endLine":29,"endColumn":2},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'listData'. Either include it or remove the dependency array.","ArrayExpression",["36"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration",{"desc":"37","fix":"38"},"Upd